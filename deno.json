{
  "compilerOptions": {
    "lib": ["deno.ns", "dom"],
    "jsx": "react-jsx"
  },
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    },
    "exclude": [
      "**/*.generated.ts"
    ]
  },
  "fmt": {
    "exclude": [
      "**/*.generated.ts"
    ]
  },
  "imports": {
    "@david/dax": "jsr:@david/dax@^0.40.0",
    "@david/dts-minify": "jsr:@david/dts-minify@^0.3.3",
    "@std/expect": "jsr:@std/expect@^0.221",
    "@std/semver": "jsr:@std/semver@^0.221"
  },
  "exclude": [
    "dist"
  ],
  "unstable": [
    "byonm",
    "sloppy-imports",
    "bare-node-builtins",
    "unsafe-proto"
  ]
}
