{
  "incremental": true,
  "lineWidth": 160,
  "indentWidth": 2,
  "includes": ["**/*.{ts,tsx,js,jsx,json,md,css}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "public/vs",
    "shared/dist",
    "site/dist",
    "site/src/resources",
    "CHANGELOG.md"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.60.0.wasm",
    "https://plugins.dprint.dev/json-0.13.2.wasm",
    "https://plugins.dprint.dev/markdown-0.11.3.wasm",
    "https://plugins.dprint.dev/prettier-0.4.0.exe-plugin@2a38bff5dff736cb53195a93d6a15a6281580205dfb0552ca0562bac8b3fc065"
  ]
}
